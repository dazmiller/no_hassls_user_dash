<%- model_class = UserProfile -%>
<div class="page-header">
  <h1><%=t '.title', :default => UserProfile %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:first_name) %></th>
      <th><%= model_class.human_attribute_name(:middle_name) %></th>
      <th><%= model_class.human_attribute_name(:surname) %></th>
      <th><%= model_class.human_attribute_name(:sex) %></th>
      <th><%= model_class.human_attribute_name(:birthdate) %></th>
      <th><%= model_class.human_attribute_name(:height) %></th>
      <th><%= model_class.human_attribute_name(:weight) %></th>
      <th><%= model_class.human_attribute_name(:smoker) %></th>
      <th><%= model_class.human_attribute_name(:address1) %></th>
      <th><%= model_class.human_attribute_name(:address2) %></th>
      <th><%= model_class.human_attribute_name(:address3) %></th>
      <th><%= model_class.human_attribute_name(:suburb) %></th>
      <th><%= model_class.human_attribute_name(:state) %></th>
      <th><%= model_class.human_attribute_name(:country) %></th>
      <th><%= model_class.human_attribute_name(:postcode) %></th>
      <th><%= model_class.human_attribute_name(:email1) %></th>
      <th><%= model_class.human_attribute_name(:email2) %></th>
      <th><%= model_class.human_attribute_name(:phone_home) %></th>
      <th><%= model_class.human_attribute_name(:phone_mob) %></th>
      <th><%= model_class.human_attribute_name(:profession) %></th>
      <th><%= model_class.human_attribute_name(:title) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.id %></td>
        <td><%= user_profile.first_name %></td>
        <td><%= user_profile.middle_name %></td>
        <td><%= user_profile.surname %></td>
        <td><%= user_profile.sex %></td>
        <td><%= user_profile.birthdate %></td>
        <td><%= user_profile.height %></td>
        <td><%= user_profile.weight %></td>
        <td><%= user_profile.smoker %></td>
        <td><%= user_profile.address1 %></td>
        <td><%= user_profile.address2 %></td>
        <td><%= user_profile.address3 %></td>
        <td><%= user_profile.suburb %></td>
        <td><%= user_profile.state %></td>
        <td><%= user_profile.country %></td>
        <td><%= user_profile.postcode %></td>
        <td><%= user_profile.email1 %></td>
        <td><%= user_profile.email2 %></td>
        <td><%= user_profile.phone_home %></td>
        <td><%= user_profile.phone_mob %></td>
        <td><%= user_profile.profession %></td>
        <td><%= user_profile.title %></td>
        <td><%=l user_profile.created_at, format: :long %></td>
        <td>
          <%= link_to user_profile_path(user_profile), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to user_profile_path(user_profile), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_user_profile_path,
            :class => 'btn btn-primary' %>

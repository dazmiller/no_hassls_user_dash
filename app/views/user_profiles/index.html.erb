<h1>Listing user_profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>First name</th>
      <th>Middle name</th>
      <th>Surname</th>
      <th>Sex</th>
      <th>Birthdate</th>
      <th>Height</th>
      <th>Weight</th>
      <th>Smoker</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Address3</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Country</th>
      <th>Postcode</th>
      <th>Email1</th>
      <th>Email2</th>
      <th>Phone home</th>
      <th>Phone mob</th>
      <th>Profession</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.user %></td>
        <td><%= user_profile.first_name %></td>
        <td><%= user_profile.middle_name %></td>
        <td><%= user_profile.surname %></td>
        <td><%= user_profile.sex %></td>
        <td><%= user_profile.birthdate %></td>
        <td><%= user_profile.height %></td>
        <td><%= user_profile.weight %></td>
        <td><%= user_profile.smoker %></td>
        <td><%= user_profile.address1 %></td>
        <td><%= user_profile.address2 %></td>
        <td><%= user_profile.address3 %></td>
        <td><%= user_profile.suburb %></td>
        <td><%= user_profile.state %></td>
        <td><%= user_profile.country %></td>
        <td><%= user_profile.postcode %></td>
        <td><%= user_profile.email1 %></td>
        <td><%= user_profile.email2 %></td>
        <td><%= user_profile.phone_home %></td>
        <td><%= user_profile.phone_mob %></td>
        <td><%= user_profile.profession %></td>
        <td><%= user_profile.title %></td>
        <td><%= link_to 'Show', user_profile %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
        <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User profile', new_user_profile_path %>

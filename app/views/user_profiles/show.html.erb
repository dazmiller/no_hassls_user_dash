<%- model_class = UserProfile -%>
<div class="page-header">
  <h1><%=t '.title', :default => UserProfile %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:first_name) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.first_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:middle_name) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.middle_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:surname) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.surname %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:sex) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.sex %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:birthdate) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.birthdate %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:height) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.height %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:weight) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.weight %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:smoker) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.smoker %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address1) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.address1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address2) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.address2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address3) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.address3 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:suburb) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.suburb %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.state %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:country) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.country %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:postcode) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.postcode %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email1) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.email1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email2) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.email2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone_home) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.phone_home %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone_mob) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.phone_mob %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:profession) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.profession %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:title) %></dt>
      <dd><pre class="prettyprint"><%= @user_profile.title %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              user_profiles_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_user_profile_path(@user_profile), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              user_profile_path(@user_profile),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

